{
  "dependencies": {
    "@types/node": "22.1.0",
    "dotenv-cli": "7.4.2",
    "prettier": "3.3.3",
    "turbo": "2.0.11",
    "typescript": "5.5.4"
  },
  "name": "@hrishikeshk/workspace",
  "packageManager": "npm@10.8.2",
  "private": true,
  "scripts": {
    "clean": "turbo run clean",
    "deploy": "dotenv -- netlify build --filter=@hrishikeshk/site --offline",
    "dev": "dotenv -- netlify dev --filter=@hrishikeshk/site --offline"
  },
  "type": "module",
  "workspaces": [
    "./packages/*"
  ]
}
