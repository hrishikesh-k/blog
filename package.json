{
  "dependencies": {
    "@biomejs/biome": "1.9.1",
    "@types/node": "22.5.5",
    "dotenv-cli": "7.4.2",
    "turbo": "2.1.2",
    "typescript": "5.6.2"
  },
  "name": "@hrishikeshk/workspace",
  "packageManager": "npm@10.8.3",
  "private": true,
  "scripts": {
    "clean": "turbo run @hrishikeshk/archive#clean",
    "deploy": "dotenv -- netlify build --filter=@hrishikeshk/site --offline",
    "dev": "dotenv -- netlify dev --filter=@hrishikeshk/site --offline"
  },
  "type": "module",
  "workspaces": [
    "./packages/*"
  ]
}
